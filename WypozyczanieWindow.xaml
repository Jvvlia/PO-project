        <TextBox x:Name="TxtTelefon" HorizontalAlignment="Left" Height="30" Margin="537,227,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="240"/>
        <TextBox x:Name="TxtNumerPrawaJazdy" HorizontalAlignment="Left" Height="30" Margin="537,262,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="240"/>
