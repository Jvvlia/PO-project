        <Label x:Name="LblTelefon" Content="Telefon:" HorizontalContentAlignment="Right" HorizontalAlignment="Left" Height="30" Margin="420,227,0,0" VerticalAlignment="Top" Width="112"/>
        <Label x:Name="LblNumerPrawaJazdy" Content="Numer prawa jazdy:" HorizontalContentAlignment="Right" HorizontalAlignment="Left" Height="30" Margin="400,262,0,0" VerticalAlignment="Top" Width="132"/>
        <TextBox x:Name="TxtImie" HorizontalAlignment="Left" Height="30" Margin="537,87,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="240"/>
        <TextBox x:Name="TxtNazwisko" HorizontalAlignment="Left" Height="30" Margin="537,122,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="240"/>
