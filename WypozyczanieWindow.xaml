        <TextBox x:Name="TxtKwota" HorizontalAlignment="Left" Height="30" Margin="144,317,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="240"/>
        <TextBox x:Name="TxtSamochod" HorizontalAlignment="Left" Height="30" Margin="144,85,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="240"/>
        <TextBox x:Name="TxtPracownik" HorizontalAlignment="Left" Height="30" Margin="144,227,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="240"/>

